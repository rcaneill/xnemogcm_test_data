	</file>
	
	<file id="file3" name_suffix="_grid_V" description="ocean V grid variables" >
	  <field field_ref="ssv"          name="vos" />
	</file>
	
      </file_group>

      <file_group id="3d" output_freq="3d"  output_level="10" enabled=".TRUE."/> <!-- 3d files -->    

