   rn_trsfac(5)  = 8.264e-02   !  (  0.021 / 31. * 122 )
   rn_trsfac(7)  = 3.313e-01     !  ( 8.8   / 28.1 )
   rn_trsfac(14) = 6.266e-04   !  (  0.035 / 55.85 )
   rn_trsfac(23) =  5.4464e-01  !  ( From kgN m-2 s-1 to molC l-1 ====> zfact = 7.625/14 )
