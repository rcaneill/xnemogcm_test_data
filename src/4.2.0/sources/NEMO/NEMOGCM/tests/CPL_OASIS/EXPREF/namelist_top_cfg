   rn_trcfac(10) = 8.333e+01   !  ( 1e3 / 12
   rn_trcfac(14) = 4.166e-03   !  ( 1e3 / 12 * 5e-5 )
   rn_trcfac(23) = 5.446e+02   !  (  1e3 / 14 * 7.625 )
