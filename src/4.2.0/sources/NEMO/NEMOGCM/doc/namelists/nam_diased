   seddiag2d(17)  = 'FlxCaCO3   ' , 'Calcite burial flux                    ',  'mol/cm2/s'
   seddiag2d(18)  = 'FlxBSi     ' , 'BSi burial flux                        ',  'mol/cm2/s'
   seddiag2d(19)  = 'FlxClay    ' , 'Clay burial flux                       ',  'mol/cm2/s'
   seddiag2d(20)  = 'Flxtot     ' , 'Total burial flux                      ',  'mol/cm2/s'
