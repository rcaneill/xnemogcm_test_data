<!-- simplest test cases : output data to file -->

  

  <file_definition  type="one_file" >
    <file id="atm_output_scalar_transformation_extract" output_freq="12ts" type="one_file" enabled="true" >
       <field field_ref="field_Z" />
       <field id="fieldZ_pos4" field_ref="field_Z" name="fieldZ_pos4" grid_ref="grid0D_extract_pos4" />
       <field id="fieldZ_pos5" field_ref="field_Z" name="fieldZ_pos5" grid_ref="grid0D_extract_pos5" />
       <field field_ref="field3D" />
       <field id="field3D_pos4" enabled="true" field_ref="field3D" name="field3D_pos4" grid_ref="grid2D_extract_pos4" />
       <field id="field3D_pos5" enabled="true" field_ref="field3D" name="field3D_pos5" grid_ref="grid2D_extract_pos5"/>
    </file>

    <file id="atm_output_scalar_transformation_reduce_axis" output_freq="4ts" type="one_file" enabled="true">
       <field field_ref="field_Z" />
       <field id="field_reduce_axis_max" field_ref="field_Z" name="field_reduce_axis_max" grid_ref="grid0D_reduce_axis_max" enabled="true"/>
       <field id="field_reduce_axis_min" field_ref="field_Z" name="field_reduce_axis_min" grid_ref="grid0D_reduce_axis_min" enabled="true"/>
       <field id="field_reduce_axis_sum" field_ref="field_Z" name="field_reduce_axis_sum" grid_ref="grid0D_reduce_axis_sum" enabled="true"/>
       <field id="field_reduce_axis_ave" field_ref="field_Z" name="field_reduce_axis_ave" grid_ref="grid0D_reduce_axis_ave" enabled="true"/>
    </file>
   
    <file id="atm_output_scalar_transformation_reduce_domain" output_freq="24ts" type="one_file" enabled="true">
       <field field_ref="field2D" />
       <field id="field_reduce_domain_max" field_ref="field2D" name="field_reduce_domain_max" grid_ref="grid2D_reduce_domain_max" />
       <field id="field_reduce_domain_min" field_ref="field2D" name="field_reduce_domain_min" grid_ref="grid2D_reduce_domain_min" />
       <field id="field_reduce_domain_sum" field_ref="field2D" name="field_reduce_domain_sum" grid_ref="grid2D_reduce_domain_sum" />
       <field id="field_reduce_domain_ave" field_ref="field2D" name="field_reduce_domain_ave" grid_ref="grid2D_reduce_domain_ave" />
    </file>



  
    <file id="atm_output" output_freq="1ts" type="one_file" enabled="false">
       <field field_ref="field3D" />
       <field field_ref="field2D" /> 
       <field field_ref="field_X" />
       <field field_ref="field_Y" />
       <field field_ref="field_XY" />
       <field field_ref="field_Z" />
       <field field_ref="field_XYZ" />
       <field field_ref="field_XZ" />
       <field field_ref="field_YZ" />
       <field field_ref="pressure"  /> 
       <field field_ref="field3D_resend" />
    </file>

    <file id="atm_output_other" output_freq="1ts" type="one_file" enabled="false">
       <field field_ref="other_field3D" />
       <field field_ref="other_field2D" /> 
       <field field_ref="other_field_X" />
       <field field_ref="other_field_Y" />
       <field field_ref="other_field_XY" />
       <field field_ref="other_field_Z" />
       <field field_ref="other_field_XYZ" />
       <field field_ref="other_field_XZ" />
       <field field_ref="other_field_YZ" />
       <field field_ref="other_pressure"  /> 
       <field field_ref="other_field3D_resend" />
    </file>

    <file id="atm_output_W" output_freq="1ts" enabled="false">
       <field field_ref="field3D_W" />
       <field field_ref="field2D_W" /> 
       <field field_ref="field_XW" />
       <field field_ref="field_YW" />
       <field field_ref="field_XYW" />
       <field field_ref="field_ZW" />
       <field field_ref="field_XYZW" />
       <field field_ref="field_XZW" />
       <field field_ref="field_YZW" />
    </file>
    
    
  </file_definition>
  
  
</context>
