<?xml version="1.0"?>
<simulation>

  <context id="nemo"
	   src="./context_nem.xml"/>
  <context id="arpsfx"
	   src="./context_arpsf.xml"/>
  
  <context id="xios">
    <variable_definition>
      <variable_group id="buffer">
        <variable id="optimal_buffer_size" type="string">performance</variable>
